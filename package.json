    "eslint-plugin-react": "^7.30.0",
    "prettier": "^2.7.1",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint",
      "pretty-quick â€” staged",
