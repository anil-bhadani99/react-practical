{
  "name": "delivery_service",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@googlemaps/react-wrapper": "^1.1.35",
    "@reduxjs/toolkit": "^1.8.2",
    "env-cmd": "^10.1.0",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
